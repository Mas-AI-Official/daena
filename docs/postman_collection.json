      "item": [
        {
          "name": "Run Council Debate",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              },
              {
                "key": "X-API-Key",
                "value": "{{api_key}}",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"topic\": \"Should we invest in new infrastructure?\",\n  \"context\": {\n    \"budget\": 100000,\n    \"timeline\": \"Q2 2025\"\n  }\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/v1/council/debate",
              "host": ["{{base_url}}"],
