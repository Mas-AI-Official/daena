{
  "info": {
    "name": "Daena AI VP API",
    "description": "Complete API collection for Daena AI VP System",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
    "version": "2.0.0"
  },
  "auth": {
    "type": "apikey",
    "apikey": [
      {
        "key": "value",
        "value": "{{api_key}}",
        "type": "string"
      },
      {
        "key": "key",
        "value": "X-API-Key",
        "type": "string"
      }
    ]
  },
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:8000",
      "type": "string"
    },
    {
      "key": "api_key",
      "value": "your-api-key",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "System",
      "item": [
        {
          "name": "Health Check",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/v1/system/health",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "system", "health"]
            }
          },
          "response": []
        },
        {
          "name": "System Summary",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "X-API-Key",
                "value": "{{api_key}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/v1/system/summary",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "system", "summary"]
            }
          },
          "response": []
        },
        {
          "name": "System Metrics",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/v1/system/metrics",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "system", "metrics"]
            }
          },
          "response": []
        }
      ]
    },
    {
      "name": "Agents",
      "item": [
        {
          "name": "Get All Agents",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/v1/agents?limit=100&offset=0",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "agents"],
              "query": [
                {
                  "key": "limit",
                  "value": "100"
                },
                {
                  "key": "offset",
                  "value": "0"
                },
                {
                  "key": "department_id",
                  "value": "",
                  "disabled": true
                },
                {
                  "key": "include_adjacency",
                  "value": "false",
                  "disabled": true
                }
              ]
            }
          },
          "response": []
        },
        {
          "name": "Get Agent by ID",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/v1/agents/:agent_id",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "agents", ":agent_id"],
              "variable": [
                {
                  "key": "agent_id",
                  "value": "agent_001"
                }
              ]
            }
          },
          "response": []
        },
        {
          "name": "Get Agent Metrics",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "X-API-Key",
                "value": "{{api_key}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/v1/monitoring/agent-metrics",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "monitoring", "agent-metrics"]
            }
          },
          "response": []
        }
      ]
    },
    {
      "name": "Daena Chat",
      "item": [
        {
          "name": "Get Daena Status",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/v1/daena/status",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "daena", "status"]
            }
          },
          "response": []
        },
        {
          "name": "Start Chat Session",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"user_id\": \"user_123\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/v1/daena/chat/start",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "daena", "chat", "start"]
            }
          },
          "response": []
        },
        {
          "name": "Send Message",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"session_id\": \"{{session_id}}\",\n  \"message\": \"What are the current priorities?\",\n  \"context\": {}\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/v1/daena/chat/message",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "daena", "chat", "message"]
            }
          },
          "response": []
        }
      ]
    },
