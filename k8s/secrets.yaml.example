apiVersion: v1
kind: Secret
metadata:
  name: daena-secrets
type: Opaque
stringData:
  database-url: "sqlite:///./data/daena.db"
  secret-key: "your-secret-key-here"
  aes-key: "your-aes-256-key-here-base64"
  openai-api-key: "your-openai-api-key"
  anthropic-api-key: "your-anthropic-api-key"
  google-api-key: "your-google-api-key"
  deepseek-api-key: "your-deepseek-api-key"
  grok-api-key: "your-grok-api-key"
  elevenlabs-api-key: "your-elevenlabs-api-key"

